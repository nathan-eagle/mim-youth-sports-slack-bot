{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "SLACK_BOT_TOKEN": "@slack_bot_token",
    "SLACK_SIGNING_SECRET": "@slack_signing_secret",
    "OPENAI_API_KEY": "@openai_api_key",
    "PRINTIFY_API_TOKEN": "@printify_api_token",
    "PRINTIFY_SHOP_ID": "@printify_shop_id"
  }
} 